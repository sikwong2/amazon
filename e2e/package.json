{
  "name": "e2e",
  "version": "1.0.0",
  "description": "e2e",
  "main": "e2e.js",
  "scripts": {
    "test": "jest"
  },
  "author": "Kevin Schultz",
  "license": "ISC",
  "dependencies": {
    "@jest/globals": "^29.7.0",
    "jest": "^29.7.0",
    "puppeteer": "^22.10.0"
  },
  "jest": {
    "testEnvironment": "node",
    "testTimeout": 30000,
    "testMatch": [
      "**/__tests__/**/*.[jt]s?(x)",
      "**/?(*.)+(spec|test).[tj]s?(x)"
    ],
    "coveragePathIgnorePatterns": [
      "/node_modules/"
    ],
    "coverageThreshold": {
      "global": {
        "branches": 80,
        "functions": 80,
        "lines": 80,
        "statements": 80
      }
    }
  }
}